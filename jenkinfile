pipeline {
    agent any
    stages {
       stage('Build and push Docker image') {
            steps {
                script {
                    sh 'docker build . -t 172.23.8.1:9500/snipeit:latest --no-cache'
                    sh 'docker image push 172.23.8.1:9500/snipeit:latest'
                }
            }
        }
    }
}
